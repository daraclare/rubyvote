<section class="container">
           
  <div class="row">

    <div class="col-md-12">

      <button><% @polls.each do |poll| %></button>

      <p><%= poll.title %></p>

      <p><%= link_to 'Show', poll %></p>
      <button class="btn btn-default"><%= link_to 'Edit', edit_poll_path(poll) %></button>
      <button class="btn btn-default"><%= link_to 'Back', poll_path(poll) %></button>
      <button class="btn btn-default"><%= link_to 'Add question', new_poll_question_path(poll) %></button>
       <button class="btn btn-default"><%= link_to 'Answer', new_poll_reply_path(poll) %></button>
      <button class="btn btn-default"><%= link_to 'Destroy', poll, method: :delete, data: { confirm: 'Are you sure?' } %></button>
      
      <% end %>

      <button class="btn btn-default"><%= link_to 'New Poll', new_poll_path %></button>


    </div>

  </div>

</section>
         

