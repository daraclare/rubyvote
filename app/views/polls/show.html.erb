


<section class="container">
           
    <div class="row">
             
		<div class="col-md-12">
		
			<p id="notice"><%= notice %></p>

			<p>
			  <strong>Title:</strong>
			  <%= @poll.title %>
			</p>

			<%= link_to 'Add question', new_poll_question_path(@poll) %> | 
			<%= link_to 'Edit', edit_poll_path(@poll) %> |
			<%= link_to 'Back', polls_path %>


			<ul>
			<% @poll.questions.each do |questions|%>
				
				<li> <%= questions.title %> </li>

				<% end %>
			</ul>

			<ul>
			<% @question.possible_answers.each do |possible_answer|%>
				
				<li> <%= possible_answer.title %> </li>

				<% end %>
			</ul>



		</div>

    </div>

</section>
         
